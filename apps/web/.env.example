# Development Environment Variables
NODE_ENV=development
NEXT_PUBLIC_APP_NAME="Brenda Cereals"
NEXT_PUBLIC_APP_VERSION="1.0.0"

# Database Configuration (PostgreSQL)
DATABASE_URL="postgresql://username:password@localhost:5432/brenda_cereals"
POSTGRES_USER=brenda_user
POSTGRES_PASSWORD=secure_password
POSTGRES_DB=brenda_cereals

# Authentication (NextAuth.js)
NEXTAUTH_URL=http://localhost:3001
NEXTAUTH_SECRET=your-super-secret-key-here

# External APIs
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret
NEXT_PUBLIC_PAYPAL_CLIENT_ID=your_paypal_client_id

# File Upload & Storage
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Email Service
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# Analytics & Monitoring
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
SENTRY_DSN=https://your-sentry-dsn.ingest.sentry.io

# Development Tools
ANALYZE=false
BUNDLE_ANALYZE=false
